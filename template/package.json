{
  "name": "koa2-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/app.js",
    "deploy": "cross-env NODE_ENV=production pm2 start src/app.js --interpreter ./node_modules/.bin/babel-node --name \"koa2Starter\" --max-memory-restart 400M",
    "socket": "cross-env NODE_ENV=develoment nodemon --exec yarn babel-node src/socket.js"
  },
  "dependencies": {
    "address": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.1",
    "colors": "^1.1.2",
    "cross-env": "^5.1.1",
    "ejs": "^2.5.7",
    "koa": "^2.3.0",
    "koa-body": "^2.5.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.1",
    "koa-websocket": "^4.0.0",
    "lodash": "^4.17.4",
    "module-alias": "^2.0.2",
    "mongoose": "^4.12.5",
    "path-to-regexp": "^2.1.0",
    "pm2": "^2.7.2",
    "query-string": "^5.0.1"
  },
  "devDependencies": {
    "chokidar": "^1.7.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.12.1",
    "readline": "^1.3.0",
    "shelljs": "^0.7.8"
  },
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=5.0.0"
  },
  "_moduleAliases": {
    "server": "./src/",
    "config": "./src/config.js",
    "db": "./src/db.js",
    "models": "./src/models/",
    "schemas": "./src/schemas/",
    "controllers": "./src/controllers/",
    "services": "./src/services/"
  }
}
